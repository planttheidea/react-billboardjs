{
  "extends": ["rapid7/browser", "rapid7/react"],
  "globals": {
    "__dirname": true,
    "global": true,
    "module": true,
    "process": true,
    "require": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "rules": {
    "object-curly-newline": 0,
    "object-curly-spacing": 0,
    "rapid7/named-import-newline": 0
  }
}
