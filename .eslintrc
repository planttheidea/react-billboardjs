{
  "env": {
    "browser": true,
    "jest": true
  },
  "extends": ["eslint:recommended"],
  "globals": {
    "__dirname": true,
    "global": true,
    "module": true,
    "process": true,
    "require": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "warn",
    "no-unused-vars": ["error", { "varsIgnorePattern": "[iI]gnored" }]
  }
}
